{
  "name": "justflight",
  "version": "0.3.0",
  "description": "A very simple Betaflight configurator.",
  "author": "Nicolas Cordin",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dindonus/justflight.git"
  },
  "keywords": [
    "justflight",
    "betaflight"
  ],
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "start": "npm run electron > /dev/null | react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "electron": "electron .",
    "postinstall": "electron-rebuild --force",
    "package": "electron-packager ./ --out=./packages"
  },
  "dependencies": {
    "antd": "^3.8.0",
    "lodash": "^4.17.10",
    "promise-queue": "^2.2.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^5.0.7",
    "react-scripts": "3.2.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "usb": "^1.6.2"
  },
  "devDependencies": {
    "electron": "^7.1.3",
    "electron-packager": "^14.1.1",
    "electron-rebuild": "^1.8.8"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devEngines": {
    "node": ">=10.0.x",
    "npm": ">=5.6.x",
    "yarn": ">=1.3.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
